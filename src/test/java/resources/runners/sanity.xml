<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="sanity" verbose="1">
    <groups>
        <run>
            <include name="sanity"></include>
        </run>
    </groups>
    <listeners>
        <listener class-name="com.automation.customlisteners.CustomListeners"></listener>
        <listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"></listener>
    </listeners>
    <test name="sanity">
        <classes>
            <class name="com.automation.testsuite.AccountPageTest"></class>
            <class name="com.automation.testsuite.SignPageTest"></class>
            <class name="com.automation.testsuite.WomenCategoryPageTest"></class>
           

        </classes>
    </test>
</suite>